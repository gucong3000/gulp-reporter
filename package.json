{
  "author": "gucong3000",
  "bugs": {
    "url": "https://github.com/gucong3000/gulp-reporter/issues"
  },
  "dependencies": {
    "buffered-spawn": "^3.3.2",
    "bufferstreams": "^1.1.1",
    "cli-truncate": "^1.0.0",
    "emphasize": "^1.4.0",
    "gulp-util": "^3.0.8",
    "lodash.get": "^4.4.2",
    "os-locale": "^2.0.0",
    "request-promise-native": "^1.0.4",
    "string-width": "^2.1.0",
    "term-size": "^1.2.0",
    "through2": "^2.0.3",
    "to-time": "^1.0.2"
  },
  "description": "Error report localization for: CSSLint/EditorConfig/ESLint/HTMLHint/JSCS/JSHint/PostCSS/TSLint",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "eclint": "^2.3.0",
    "eslint": "^4.2.0",
    "goo.gl": "^0.1.4",
    "gulp-csslint": "^1.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-html-postcss": "^7.0.1",
    "gulp-htmlhint": "^0.3.1",
    "gulp-jscs": "^4.0.0",
    "gulp-jshint": "^2.0.4",
    "gulp-tslint": "^8.1.1",
    "jsdom": "^11.1.0",
    "jshint": "^2.9.5",
    "json-stable-stringify": "^1.0.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "sinon": "^2.3.8",
    "stylelint": "^7.13.0",
    "stylelint-config-standard": "^16.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "vinyl": "^2.1.0",
    "vinyl-fs": "^2.4.4"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/gucong3000/gulp-reporter#readme",
  "keywords": [
    "gulp",
    "reporter",
    "report",
    "localization",
    "CSSLint",
    "ECLint",
    "EditorConfig",
    "ESLint",
    "HTMLHint",
    "JSCS",
    "JSHint",
    "PostCSS",
    "TSLint",
    "git",
    "blame",
    "author"
  ],
  "license": "MIT",
  "main": "./lib/gulp-plugin.js",
  "name": "gulp-reporter",
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 85,
    "exclude": [
      "lib/js-browser-reporter.js",
      "test/**/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gucong3000/gulp-reporter.git"
  },
  "scripts": {
    "test_lang": "cross-env LANG=zh_CN npm run mocha -- --color && cross-env LANG=en_US npm run mocha -- --no-color",
    "coveralls": "coveralls < coverage/lcov.info",
    "mocha": "mocha test/*.test.js --no-timeouts",
    "pretest": "eslint lib test/*.test.js && node srcipts/shorturl",
    "test": "nyc npm run test_lang"
  },
  "version": "2.1.0"
}
